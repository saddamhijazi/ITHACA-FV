FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;

// Number of modes to output and to use for projection
N_modes_T 25;
N_modes_DEIM_A 40;
N_modes_DEIM_B 40;

N_modes_Geo 40;
N_modes_DEIM_A_GEO 40;
N_modes_DEIM_B_GEO 40;

// Output format to save market vectors.
OutPrecision 20;
OutType fixed;


